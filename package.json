  "czConfig": {
    "path": "node_modules/cz-conventional-changelog"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run test:single"
    }
  },
  "dependencies": {
    "data.validation": "^1.3.1",
    "ramda": "^0.26.1",
    "rxjs": "^6.5.1"
  }
}
