    "resolve-url-loader": "^1.6.0",
    "rimraf": "^2.5.4",
    "selenium-webdriver": "^4.0.0-alpha.1",
    "semantic-release": "^8.2.3",
    "semistandard": "^11.0.0",
    "uglifyjs-webpack-plugin": "^1.2.7",
    "url-loader": "^0.6.2",
    "webpack": "^4.29.3",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.2.3",
    "webpack-combine-loaders": "^2.0.4",
    "webpack-dev-middleware": "^3.5.2",
