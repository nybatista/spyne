{
  "name": "spyne",
  "sideEffects": false,
  "version": "0.10.19",
  "description": "Modern, and efficient javascript frontend framework",
  "main": "lib/spyne.min.js",
  "module": "src/spyne/spyne.js",
  "scripts": {
    "eslint-fix-dry": "eslint 'src/spyne/**' --fix-dry-run",
    "eslint-fix": "eslint --fix  'src/spyne/**'",
    "eslint-fix-tests": "eslint --fix  'src/tests/**'",
    "report-coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "commit": "git-cz",
    "build": "webpack --mode production   --env build --display-modules  ",
    "start": "webpack-dev-server --hot ",
    "dev": "webpack --mode development --progress --colors --watch --env dev ",
    "stats": "webpack --profile --json --optimize-minimize  --mode production --env build  > stats.json",
    "docs": "jsdoc --readme README.md -d docs  -c docs/jsdoc.config.json -r",
    "esdocs": "esdoc -d esdocs  -c esdocs/esdoc.config.json -r",
    "test": "karma start",
    "test:single": "npm test -- --single-run",
    "watch:test": "npm test -- --auto-watch --no-single-run",
    "test:mocha": "mocha tests"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/spynejs/spyne.git"
  },
  "keywords": [],
  "author": "Frank Batista",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.4.0",
    "@babel/preset-env": "^7.4.1",
    "babel": "^6.23.0",
    "babel-cli": "^7.0.0-beta.3",
    "babel-core": "^6.26.0",
    "babel-eslint": "7.2.3",
    "babel-loader": "^8.0.0-beta.4",
    "babel-minify-webpack-plugin": "^0.3.1",
    "babel-plugin-__coverage__": "^11.0.0",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-plugin-discard-module-references": "^1.1.2",
    "babel-polyfill": "^6.23.0",
    "babel-preset-2017": "0.0.0",
    "babel-preset-env": "^2.0.0-beta.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2016": "^6.24.1",
    "babel-preset-es2017": "^6.16.0",
    "babel-preset-stage-2": "^6.24.1",
    "chai": "^4.2.0",
    "chai-dom": "^1.8.1",
    "classlist-polyfill": "^1.0.3",
    "clean-webpack-plugin": "^0.1.19",
    "commitizen": "^3.0.7",
    "cpy-cli": "^1.0.1",
    "css-loader": "^2.1.1",
    "cz-conventional-changelog": "^2.1.0",
    "es6-promise": "^4.2.4",
    "eslint": "^5.15.3",
    "eslint-config-semistandard": "^13.0.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-standard": "^4.0.0",
    "expect": "^1.20.2",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^1.1.11",
    "ghooks": "^2.0.4",
    "history": "^4.4.0",
    "immutable": "^3.8.1",
    "istanbul": "^0.4.5",
    "jsdoc": "^3.5.5",
    "json-loader": "^0.5.4",
    "karma": "^4.0.1",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.2",
    "karma-jasmine-es6": "0.0.3",
    "karma-mocha": "^1.3.0",
    "karma-webpack": "^3.0.5",
    "marked": "^0.6.2",
    "mocha": "^5.2.0",
    "mocha-loader": "^2.0.0",
    "mocha-webpack": "^1.1.0",
